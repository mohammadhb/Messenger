{
  "name": "messenger-bot",
  "version": "0.0.1",
  "description": "A Simple Messenger Bot",
  "main": "index.js",
  "private":true,
  "scripts": {
    "start": "node index.js",
    "doc": "swagger-jsdoc -d ./swagger.config.js -o ./swagger.json './database/mongodb/models/**/*.js' './controller/**/*.js' && redoc-cli serve ./swagger.json",
    "document": "npm run swagger",
    "cov": "npm test -- --coverage",
    "lint": "eslint './**/*.js'",
    "format": "npm run lint -- --fix",
    "test": "jest --runInBand",
    "dev": "nodemon index.js"
  },
  "keywords": [
    "messenger",
    "bot",
    "facebook"
  ],
  "author": "Mohammad Hosein Balkhani",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.12.3",
    "redis": "^3.1.0",
    "sequelize": "^6.6.2",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "redoc-cli": "^0.11.4",
    "swagger-jsdoc": "^6.1.0"
  }
}
